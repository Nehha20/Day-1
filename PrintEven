#include<stdio.h>
void PrintEven(int iNo)
{
    if(iNo <= 0)
    {
        return;
    }
    
    int current_number = 2;
    for(int i = 0; i < iNo; i++)
    {
        printf("%d ", current_number);
        current_number += 2;
    }
    printf("\n");
}

int main()
{
    int iValue = 0;
    printf("Enter number\n");
    scanf("%d", &iValue);
    PrintEven(iValue);
    return 0;
}
